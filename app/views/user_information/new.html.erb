<h1>Your Information</h1>
<%= form_for :user_information, url: profile_create_path(@user) do |f| %>
	<p>
    	<%= f.label :name %><br>
    	<%= f.text_field :name %>
  	</p>

  	<p>
    	<%= f.label :description %><br>
    	<%= f.text_area :description %>
  	</p>

  	<p>
    	<%= f.submit %>
  	</p>

<% end %>